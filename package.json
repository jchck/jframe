    "cssnano": "^4.1.9",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.5.0",
    "postcss-url": "^8.0.0"
  },
  "dependencies": {
    "colors.css": "^2.3.0",
    "normalize.css": "^3.0.3"
  }
}
